{
    "env": {
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-mixed-spaces-and-tabs": 0,
        "react/jsx-filename-extension":[
            "warn",
            {
                "extensions": [".js", ".jsx"]
            }
        ],
        "react/react-in-jsx-scope": "off",
        "comma-dangle": "off"
    },
    "globals": {
    "localStorage": true,
    "document": true
  }
}
